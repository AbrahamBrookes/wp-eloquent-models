        "source": "https://github.com/mallardduck/wordpress-eloquent"
    },
    "authors": [
        {
            "name": "Dan Pock",
            "email": "self@danpock.me"
        },
        {
            "name": "Drew J Bartlett",
            "email": "drewjbartlett@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "^5.4"
