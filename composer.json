{
    "name": "mallardduck/wordpress-eloquent",
    "description": "A Laravel wrapper for wordpress which turns all Wordpress models into Laravel Eloquent Models.",
    "keywords": ["wordpress", "eloquent", "laravel"],
    "homepage": "https://github.com/mallardduck/wordpress-eloquent",
    "support": {
        "issues": "https://github.com/mallardduck/wordpress-eloquent/issues",
        "source": "https://github.com/mallardduck/wordpress-eloquent"
    },
    "authors": [
        {
            "name": "Dan Pock",
            "email": "self@danpock.me"
        },
        {
            "name": "Drew J Bartlett",
            "email": "drewjbartlett@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/database": "^5.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpcompatibility/php-compatibility": "*"
    },
    "autoload": {
        "psr-4": {
            "WPEloquent\\": "src/"
        }
    },
    "scripts": {
        "phpcs": [
            "./vendor/bin/phpcs --standard=PSR12 ./src"
        ],
        "cbf": [
            "./vendor/bin/phpcbf --standard=PSR12 ./src"
        ],
        "post-install-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility",
        "post-update-cmd" : "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility"
    },
    "replace": {
        "drewjbartlett/wordpress-eloquent": "0.2.*",
        "tareq1988/wp-eloquent": "1.*"
    },
    "license": "MIT",
    "minimum-stability": "dev"
}
